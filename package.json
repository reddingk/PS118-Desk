    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-map-gl": "^4.0.9",
    "react-scripts": "^2.1.3",
    "react-sidebar": "^3.0.2",
    "socket.io-client": "^2.2.0",
    "systeminformation": "^4.27.11",
    "wait-on": "^3.2.0"
  },
